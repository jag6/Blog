<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ('../components/css.ejs') %>
    <link rel="canonical" href="website.com/contact">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta property="og:locale" content="en_US"/>
    <!-- Page Data -->
    <title>Contact</title>
    <meta name="description" content="">
    <!-- Twitter MetaData -->
    <meta name="twitter:title" content="">
    <meta name="twitter:description" content="">
    <meta name="twitter:image" content="">
    <meta name="twitter:card" content="summary_large_image">
    <!-- Other Social Media MetaData -->
    <meta property="og:title" content="">
    <meta property="og:description" content="">
    <meta property="og:image" content="">
    <meta property="og:url" content="">
    <meta property="og:site_name" content="Blogtopia">
    <!-- Scripts -->
    <%- include ('../components/scripts.ejs') %>
</head>
<body>
    <%- include('../components/header') %>
    <main>
        <section class="myContainer">
            <section class="top">
                <h1>CONTACT ME</h1>
            </section>
            <% users.forEach(user => { %>
               
                <h2><%= user.first_name %></h2>
                <h2><%= user.last_name %></h2>
                <h3><%= user.isAdmin %></h3>
                <h4><%= user.password %></h4>
                <h5 id="user-info"></h5>
                <form action="/contact/<%= user.id %>?_method=delete" method="post" class="d-inline">
                    <button type="submit" class="btn red">DELETE</button>
                </form>
                        
                    
                
            <% }) %>
        </section>
    </main>
    <%- include('../components/footer') %>
</body>
</html>